{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "moduleResolution": "bundler",
    "jsx": "preserve",
    "allowJs": true,
    "module": "esnext",
    "noEmit": true,
    "incremental": true,
    "resolveJsonModule": true,
    "types": ["@types/node"]
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
